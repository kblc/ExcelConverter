<Window x:Class="ExelConverterLite.View.ReExportProgressView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ReExportProgressView" Height="60" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner"
        DataContext="{Binding ReExportProgress, Source={StaticResource Locator}}"
        WindowStyle="None" Background="#DDDDDD">
    <GroupBox Header="{Binding ProgressText}" Margin="8">
        <ProgressBar Value="{Binding ProgressValue}" Margin="4"/>
    </GroupBox>
</Window>
